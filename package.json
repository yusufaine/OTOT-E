{
  "name": "task-e-otot",
  "version": "1.0.0",
  "description": "Task E - OTOT",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "redis": "^4.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.15",
    "eslint": "^8.23.0",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsx": "^3.9.0",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "dev": "yarn redis:restart && tsx watch ./src/index.ts",
    "redis:start": "docker compose up local-redis -d",
    "redis:rm": "docker compose rm local-redis -s -f -v",
    "redis:restart": "yarn redis:rm && yarn redis:start",
    "shut": "yarn redis:rm",
    "start": "yarn build && node ./dist/index.js"
  }
}
